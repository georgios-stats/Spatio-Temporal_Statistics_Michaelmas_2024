     <mo>(</mo>
     <mrow>
      <munder>
       <mo>&#x220F;</mo>
       <mrow>
        <mi>i</mi>
        <mo>&#x2208;</mo>
        <mstyle mathvariant='script'>
         <mi>S</mi>
        </mstyle>
       </mrow>
      </munder>
      <munder>
       <mo>Pr</mo>
       <mi>i</mi>
      </munder>
