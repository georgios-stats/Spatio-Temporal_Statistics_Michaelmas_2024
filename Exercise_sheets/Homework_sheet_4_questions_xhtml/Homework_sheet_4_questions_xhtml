    <mn>2</mn>
   </msub>
  </mrow>
  <mo>)</mo>
 </mrow>
 <mtext>d</mtext>
 <msub>
  <mi>x</mi>
  <mn>1</mn>
 </msub>
 <mtext>d</mtext>
 <msub>
  <mi>x</mi>
  <mn>2</mn>
 </msub>
 <mo stretchy='false'>=</mo>
 <msub>
  <mo>&#x222B;</mo>
  <msup>
   <mstyle mathvariant='double-struck'>
    <mi>R</mi>
   </mstyle>
   <mn>2</mn>
  </msup>
 </msub>
 <mo>exp</mo>
 <mrow>
  <mo>(</mo>
  <mrow>
   <mo stretchy='false'>-</mo>
