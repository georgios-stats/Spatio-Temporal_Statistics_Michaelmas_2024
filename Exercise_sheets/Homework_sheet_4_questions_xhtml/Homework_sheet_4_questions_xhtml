 <mtext>N</mtext>
 <mrow>
  <mo>(</mo>
  <mrow>
   <mi>X</mi>
   <mi>&#x3B2;</mi>
   <mo stretchy='false'>,</mo>
   <msup>
    <mrow>
     <mo>(</mo>
     <mrow>
