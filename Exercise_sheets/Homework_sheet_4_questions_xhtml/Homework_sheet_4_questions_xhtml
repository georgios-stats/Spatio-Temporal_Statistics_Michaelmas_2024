  <mi>&#x3C3;</mi>
  <mn>2</mn>
 </msup>
 <mo>&#x223C;</mo>
 <mtext>N</mtext>
 <mrow>
  <mo>(</mo>
  <mrow>
   <msub>
    <mrow>
     <mo>[</mo>
     <mrow>
      <mi>X</mi>
      <mi>&#x3B2;</mi>
     </mrow>
     <mo>]</mo>
    </mrow>
    <mi>i</mi>
   </msub>
   <mo stretchy='false'>+</mo>
